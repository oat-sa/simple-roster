{
    "name": "oat-sa/simple-roster",
    "type": "project",
    "description": "REST back-end service that handles authentication and eligibilities",
    "license": "GPL-2.0",
    "require": {
        "php": "^7.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-openssl": "*",
        "ext-redis": "*",
        "ext-simplexml": "*",
        "aws/aws-sdk-php": "^3.81",
        "aws/aws-sdk-php-symfony": "^2.0",
        "blackfire/php-sdk": "^1.23",
        "doctrine/annotations": "^1.6",
        "doctrine/doctrine-bundle": "^2.0",
        "doctrine/orm": "^2.7",
        "lcobucci/jwt": "^3.3",
        "league/csv": "^9.0",
        "league/flysystem-aws-s3-v3": "1.0.27",
        "league/flysystem-bundle": "^1.5",
        "nelmio/cors-bundle": "^2.0",
        "nesbot/carbon": "^2.11",
        "oat-sa/bundle-lti1p3": "^6.0",
        "sensio/framework-extra-bundle": "^5.2",
        "symfony/amazon-sqs-messenger": "^5.0",
        "symfony/apache-pack": "^1.0",
        "symfony/browser-kit": "^5.0",
        "symfony/console": "^5.0",
        "symfony/doctrine-bridge": "5.2.6",
        "symfony/dotenv": "^5.0",
        "symfony/flex": "^1.1",
        "symfony/framework-bundle": "^5.0",
        "symfony/messenger": "^5.0",
        "symfony/monolog-bundle": "^3.0",
        "symfony/process": "^5.0",
        "symfony/property-access": "^5.0",
        "symfony/property-info": "^5.0",
        "symfony/proxy-manager-bridge": "^5.0",
        "symfony/security-bundle": "^5.0",
        "symfony/serializer": "^5.0",
        "symfony/stopwatch": "^5.0",
        "symfony/twig-bundle": "^5.2",
        "symfony/uid": "^5.0",
        "symfony/validator": "^5.0",
        "symfony/yaml": "^5.0",
        "twig/twig": "^3.0"
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php56": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php71": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "require-dev": {
        "ext-intl": "*",
        "doctrine/doctrine-fixtures-bundle": "^3.1",
        "ergebnis/composer-normalize": "2.9",
        "hautelook/alice-bundle": "^2.3",
        "infection/infection": "^0.18",
        "league/flysystem-memory": "^1.0",
        "phpmd/phpmd": "^2.8",
        "phpstan/extension-installer": "^1.0",
        "phpstan/phpstan": "^0.12",
        "phpstan/phpstan-doctrine": "^0.12",
        "phpstan/phpstan-phpunit": "^0.12",
        "phpstan/phpstan-symfony": "^0.12",
        "psalm/plugin-phpunit": "^0.15.1",
        "psalm/plugin-symfony": "^2.1",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/debug-bundle": "^5.0",
        "symfony/phpunit-bridge": "^5.0",
        "symfony/profiler-pack": "^1.0",
        "symfony/var-dumper": "^5.0",
        "vimeo/psalm": "^4.6"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "^5.0"
        }
    },
    "autoload": {
        "psr-4": {
            "OAT\\SimpleRoster\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OAT\\SimpleRoster\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
